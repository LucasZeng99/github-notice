{
    "manifest_version": 2,
    "name": "Github Notice",
    "version": "0.1.0",
  
    "default_locale": "en",
    "description": "An extension for more detailed github activities",

    "browser_action": {
        "default_icon": {                    
          "16": "images/icon16.png",
          "24": "images/icon24.png",
          "32": "images/icon32.png"
        },
        "default_title": "Github Notice",
        "default_popup": "popup.html"        
      }
}